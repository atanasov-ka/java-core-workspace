
public class Student {
	private final int grade;
	private final String name;
	public Student(int grade, String name)
	{
		this.grade = grade;
		this.name = name;
	}
	
	public Student(Student copyFrom) {
		this(copyFrom.getGrade(), copyFrom.getName());
	}
	
	public String getName() {
		return name;
	}
	
	public int getGrade() {
		return grade;
	}
	
	
	
	
}
