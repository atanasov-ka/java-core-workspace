
public class Student {
	private int grade;
	private String name;
	public Student(int grade, String name)
	{
		this.grade = grade;
		this.setName(name);
	}
	
	public Student(Student copyFrom) {
		this(copyFrom.getGrade(), copyFrom.getName());
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
	
	
	
}
